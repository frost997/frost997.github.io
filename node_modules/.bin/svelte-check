#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/USER/Downloads/college-store-svelte5/college-store-v5/node_modules/.pnpm/svelte-check@4.3.5_picomatch@4.0.3_svelte@5.46.1_typescript@5.9.3/node_modules/svelte-check/bin/node_modules:/mnt/c/Users/USER/Downloads/college-store-svelte5/college-store-v5/node_modules/.pnpm/svelte-check@4.3.5_picomatch@4.0.3_svelte@5.46.1_typescript@5.9.3/node_modules/svelte-check/node_modules:/mnt/c/Users/USER/Downloads/college-store-svelte5/college-store-v5/node_modules/.pnpm/svelte-check@4.3.5_picomatch@4.0.3_svelte@5.46.1_typescript@5.9.3/node_modules:/mnt/c/Users/USER/Downloads/college-store-svelte5/college-store-v5/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/USER/Downloads/college-store-svelte5/college-store-v5/node_modules/.pnpm/svelte-check@4.3.5_picomatch@4.0.3_svelte@5.46.1_typescript@5.9.3/node_modules/svelte-check/bin/node_modules:/mnt/c/Users/USER/Downloads/college-store-svelte5/college-store-v5/node_modules/.pnpm/svelte-check@4.3.5_picomatch@4.0.3_svelte@5.46.1_typescript@5.9.3/node_modules/svelte-check/node_modules:/mnt/c/Users/USER/Downloads/college-store-svelte5/college-store-v5/node_modules/.pnpm/svelte-check@4.3.5_picomatch@4.0.3_svelte@5.46.1_typescript@5.9.3/node_modules:/mnt/c/Users/USER/Downloads/college-store-svelte5/college-store-v5/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
